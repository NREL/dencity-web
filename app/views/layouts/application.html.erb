<!DOCTYPE html>
<html>
<head>
  <title>DEncity</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="fluid-container">
  <%= render 'layouts/nav' %>
  <div class="row-fluid header-row">
    <div class="span12">
      <h1 class="sitename"><%= link_to 'DEnCity', root_path, :class => 'sitename' %></h1>
    </div>

  </div>
  <div id="blue-line-and-fade" class="row-fluid header-row">

    <div class="span12">
      <hr class='top-blue'>
      <div class="below-header"></div>
    </div>
  </div>

  <div class="row-fluid header-row">
    <!-- notices -->
    <%= render 'layouts/flash_messages' if !flash.empty? %>
  </div>
  <%= yield %>

</div> <!-- /container -->

</body>
</html>

