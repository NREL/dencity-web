<h1>Listing measure_instances</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @measure_instances.each do |measure_instance| %>
      <tr>
        <td><%= link_to 'Show', measure_instance %></td>
        <td><%= link_to 'Edit', edit_measure_instance_path(measure_instance) %></td>
        <td><%= link_to 'Destroy', measure_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Measure instance', new_measure_instance_path %>
